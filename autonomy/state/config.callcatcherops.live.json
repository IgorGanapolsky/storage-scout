{
  "mode": "live",
  "company": {
    "name": "CallCatcher Ops",
    "website": "https://callcatcherops.com",
    "intake_url": "https://callcatcherops.com/callcatcherops/intake.html",
    "reply_to": "hello@callcatcherops.com",
    "mailing_address": "11909 Glenmore Drive, Coral Springs, FL 33071",
    "signature": "\u2014 CallCatcher Ops",
    "booking_url": "https://calendly.com/igorganapolsky/audit-call",
    "baseline_example_url": "https://callcatcherops.com/callcatcherops/baseline-example.pdf"
  },
  "agents": {
    "outreach": {
      "agent_id": "agent.outreach.v1",
      "permissions": [
        "lead.read",
        "lead.write",
        "email.send"
      ],
      "daily_send_limit": 25,
      "min_score": 60,
      "target_services": [
        "med spa",
        "dentist",
        "plumber",
        "chiropractor",
        "hvac",
        "roofing",
        "locksmith",
        "pest control"
      ],
      "allowed_email_methods": [
        "direct",
        "scrape"
      ],
      "blocked_local_parts": [
        "hello",
        "office",
        "support",
        "sales",
        "service",
        "team",
        "admin",
        "appointments",
        "booking",
        "inquiries"
      ],
      "bounce_pause": {
        "enabled": true,
        "window_days": 7,
        "threshold": 0.25,
        "min_emailed": 20
      },
      "followup": {
        "enabled": true,
        "min_days_since_last_email": 3,
        "max_emails_per_lead": 5,
        "daily_send_limit": 15
      }
    },
    "observer": {
      "observe_threshold": 3,
      "reflect_threshold": 5
    }
  },
  "lead_sources": [
    {
      "type": "csv",
      "path": "autonomy/state/leads_callcatcherops_real.csv",
      "source": "callcatcherops_live"
    }
  ],
  "email": {
    "provider": "smtp",
    "smtp_host": "smtp.fastmail.com",
    "smtp_port": 587,
    "smtp_user": "hello@callcatcherops.com",
    "smtp_password_env": "SMTP_PASSWORD"
  },
  "compliance": {
    "include_unsubscribe": true,
    "unsubscribe_url": "https://callcatcherops.com/unsubscribe.html?email={{email}}",
    "can_spam_required": true
  },
  "storage": {
    "sqlite_path": "autonomy/state/autonomy_live.sqlite3",
    "audit_log": "autonomy/state/audit_live.jsonl"
  }
}