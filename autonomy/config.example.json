{
  "mode": "dry-run",
  "company": {
    "name": "CallCatcher HQ",
    "website": "https://callcatcherhq.com",
    "intake_url": "https://callcatcherhq.com/intake",
    "reply_to": "hello@callcatcherhq.com",
    "mailing_address": "123 Main St, City, ST 00000",
    "signature": "â€” CallCatcher HQ"
  },
  "agents": {
    "outreach": {
      "agent_id": "agent.outreach.v1",
      "permissions": ["lead.read", "lead.write", "email.send"],
      "daily_send_limit": 30,
      "min_score": 50,
      "allowed_email_methods": ["direct", "scrape"],
      "blocked_local_parts": ["info", "contact", "hello", "office", "support", "sales", "service", "team", "admin"],
      "bounce_pause": { "enabled": true, "window_days": 7, "threshold": 0.25, "min_emailed": 20 },
      "followup": {
        "enabled": true,
        "min_days_since_last_email": 2,
        "max_emails_per_lead": 3,
        "daily_send_limit": 15
      }
    }
  },
  "lead_sources": [
    {
      "type": "csv",
      "path": "autonomy/data/leads.csv",
      "source": "manual"
    }
  ],
  "email": {
    "provider": "smtp",
    "smtp_host": "smtp.example.com",
    "smtp_port": 587,
    "smtp_user": "hello@callcatcherhq.com",
    "smtp_password_env": "SMTP_PASSWORD"
  },
  "compliance": {
    "include_unsubscribe": true,
    "unsubscribe_url": "https://callcatcherhq.com/unsubscribe?email={{email}}",
    "can_spam_required": true
  },
  "storage": {
    "sqlite_path": "autonomy/state/autonomy.sqlite3",
    "audit_log": "autonomy/state/audit.jsonl"
  }
}
